# Disguise as FF on macOS 11.2
user-agent = "Mozilla/5.0 (Macintosh; Intel Mac OS X 11.2; rv:86.0) Gecko/20100101 Firefox/86.0"

# When following a redirect, automatically set the previous URL as referer.
referer = ";auto"

# Retrying
connect-timeout = 60
max-time 120
retry 3
retry-delay 0
retry-max-time 60
